<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bashkir.auto_school">

    <application
        android:name=".AutoSchoolApplication"
        android:allowBackup="true"
        android:icon="@mipmap/car"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AutoSchool">
        <activity android:name=".student.activities.SignUpActivity"></activity>
        <activity android:name=".student.activities.MainStudentActivity" />
        <activity android:name=".start.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>